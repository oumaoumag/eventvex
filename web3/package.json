{
  "name": "@eventvex/web3",
  "version": "1.0.0",
  "description": "EventVex Web3 smart contracts and blockchain infrastructure",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "clean": "hardhat clean",
    "typechain": "hardhat typechain",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy/01-deploy-core.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy/01-deploy-core.js --network baseSepolia",
    "deploy:additional": "hardhat run scripts/deploy/02-deploy-additional.js --network baseSepolia",
    "deploy:additional:mainnet": "hardhat run scripts/deploy/02-deploy-additional.js --network baseMainnet",
    "configure": "hardhat run scripts/deploy/03-configure-contracts.js --network baseSepolia",
    "configure:mainnet": "hardhat run scripts/deploy/03-configure-contracts.js --network baseMainnet",
    "deploy:all": "npm run deploy:testnet && npm run deploy:additional",
    "deploy:all:mainnet": "npm run deploy:mainnet && npm run deploy:additional:mainnet && npm run configure:mainnet",
    "deploy:mainnet": "hardhat run scripts/deploy/01-deploy-core.js --network baseMainnet",
    "deploy:mainnet:full": "hardhat run scripts/deploy/04-deploy-mainnet.js --network baseMainnet",
    "update:env": "node scripts/deploy/05-update-env.js",
    "update:env:mainnet": "node scripts/deploy/05-update-env.js baseMainnet",
    "deploy:recovery": "hardhat run scripts/deploy/06-recovery-deploy.js --network baseMainnet",
    "deploy:individual": "node scripts/deploy/07-individual-deploy.js",
    "deploy:individual:mainnet": "CONTRACT_NAME=EventFactory hardhat run scripts/deploy/07-individual-deploy.js --network baseMainnet",
    "deploy:factory:mainnet": "CONTRACT_NAME=EventFactory hardhat run scripts/deploy/07-individual-deploy.js --network baseMainnet",
    "deploy:access:mainnet": "CONTRACT_NAME=EventVexAccessControl hardhat run scripts/deploy/07-individual-deploy.js --network baseMainnet",
    "deploy:marketplace:mainnet": "CONTRACT_NAME=TicketMarketplace hardhat run scripts/deploy/07-individual-deploy.js --network baseMainnet",
    "deploy:paymaster:mainnet": "CONTRACT_NAME=EventVexPaymaster hardhat run scripts/deploy/07-individual-deploy.js --network baseMainnet",
    "verify:testnet": "hardhat run scripts/verify/verify-contracts.js --network baseSepolia",
    "verify:mainnet": "hardhat run scripts/verify/verify-contracts.js --network baseMainnet",
    "verify:mainnet:full": "hardhat run scripts/verify/verify-mainnet.js --network baseMainnet",
    "flatten": "hardhat flatten",
    "size": "hardhat size-contracts",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write 'contracts/**/*.sol'",
    "prepare": "npm run compile && npm run typechain"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "nft",
    "tickets",
    "events",
    "base",
    "farcaster"
  ],
  "author": "EventVex Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "chai": "^4.2.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.0",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "dotenv": "^16.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventvex/eventvex.git",
    "directory": "web3"
  },
  "bugs": {
    "url": "https://github.com/eventvex/eventvex/issues"
  },
  "homepage": "https://eventvex.com"
}
